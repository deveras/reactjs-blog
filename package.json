{
  "name": "reactjs-blog",
  "version": "1.0.0",
  "description": "Personal blog done with reactJS",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint \"$(pwd)/**/*.js\" --write",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deveras/reactjs-blog.git"
  },
  "keywords": [
    "reactJS",
    "blog"
  ],
  "author": "Andre R. Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deveras/reactjs-blog/issues"
  },
  "homepage": "https://github.com/deveras/reactjs-blog#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0"
  }
}
